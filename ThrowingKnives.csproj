<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup Condition="'$(Configuration)'=='Release'">
		<DebugSymbols>False</DebugSymbols>
		<DebugType>None</DebugType>
		<GenerateDependencyFile>false</GenerateDependencyFile>
		<PublishDir>./bin/ThrowingKnives/</PublishDir>
	</PropertyGroup>
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="CounterStrikeSharp.API" Version="1.0.362">
			<PrivateAssets>none</PrivateAssets>
			<ExcludeAssets>runtime</ExcludeAssets>
			<IncludeAssets>compile; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<Reference Include="CS2-GameHUDAPI">
			<HintPath>shared/CS2-GameHUDAPI/shared/CS2-GameHUDAPI/CS2-GameHUDAPI.dll</HintPath>
		</Reference>
	</ItemGroup>
</Project>